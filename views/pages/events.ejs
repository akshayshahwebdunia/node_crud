<div class="jumbotron text-center">
  <h1>All Events</h1>
</div>
<table class="table table-striped table-bordered table-hover">
  <th>Name</th>
  <th>Description</th>
  <th>Actions</th>
  <% for (var event of events) { %>
  <tr>
    <td><%= event.name %></td>
    <td><%= event.description %></td>
    <td>
      <a href="/events/<%= event.slug %>" class="btn btn-sm btn-primary">View</a>
    </td>
  </tr>
  <% } %>
</table>
